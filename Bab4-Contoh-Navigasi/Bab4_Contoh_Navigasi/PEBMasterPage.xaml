             x:Class="Bab4_Contoh_Navigasi.PEBMasterPage" Title="PEB">
    <MasterDetailPage.Master>
        <local:PEBMasterDetailMenu x:Name="pebMasterDetailMenu" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:PEBMasterDetailContact />
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>